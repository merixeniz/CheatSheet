1. ssh git@10.0.50.100
2. C:/Users/git/repos/
3. mkdir repoName.git
4. cd repoName.git
5. git init --bare --shared
6. icacls "C:/Users/git/repos/repoName.git" /grant git:(OI)(CI)F /T
7. exit.

